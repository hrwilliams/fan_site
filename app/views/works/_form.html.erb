<%= render('layouts/errors', :object => @work) %>

<%= form_for [@subgenre, @work] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :artist %>
  <%= f.text_field :artist %>

  <%= f.label :year_created %>
  <%= f.number_field :year_created, :max => 2016, :min => 1000 %>

  <%= f.label :description %>
  <%= f.text_field :description %>
  <p><%= f.submit %></p>
<% end %>
